{
  "description": "The project is the heart of the ThrustCalculator. Once you have a project, you can attach segments, offsets, and generate reports.",
  "type": "object",
  "title": "Calculator Project",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "description": {
      "type": [
        "string",
        "null"
      ]
    },
    "updated_at": {
      "type": "string",
      "format": "date"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "currency_symbol": {
      "type": "string",
      "default": "usd",
      "description": "Must be a valid ISO 4217 currency code."
    },
    "name": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "description",
    "updated_at",
    "created_at",
    "currency_symbol",
    "name"
  ]
